<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sydh.rule.mapper.RuleChainMapper">
        <resultMap type="RuleChain" id="RuleChainResult">
        <result property="id"    column="id"    />
        <result property="tenantId"    column="tenant_id"    />
        <result property="applicationName"    column="application_name"    />
        <result property="chainId"    column="chain_id"    />
        <result property="chainName"    column="chain_name"    />
        <result property="elData"    column="el_data"    />
        <result property="enable"    column="enable"    />
        <result property="contextBeans"    column="context_beans"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectRuleChainVo">
        select tenant_id, id, application_name, chain_id, chain_name, el_data, enable, context_beans, create_by, create_time, update_by, update_time, del_flag from rule_chain
    </sql>



    </mapper>
